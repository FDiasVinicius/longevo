CREATE TABLE chamado (
    id integer NOT NULL,
    titulo character varying(100) NOT NULL,
    descricao text,
    status integer,
    data_criacao timestamp without time zone DEFAULT date_trunc('second'::text, now()),
    id_pedido integer DEFAULT 0 NOT NULL,
    id_cliente integer NOT NULL
);


