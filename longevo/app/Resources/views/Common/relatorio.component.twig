<div class="container" id="relatorioContainer">            
  <table class="table">
    <thead>
      <tr>
      	{% for th in header %}
      		<th>{{ th }}</th>
      	{% endfor %}
      </tr>
    </thead>
    <tbody>
      {% for tr in body %}
          <tr>
            {% for td in tr %}
          		<td>{{ td|raw }}</td>
          	{% endfor %}
          </tr>
      {% endfor %}
    </tbody>
  </table>
</div>