<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RelPedidoChamado" table="rel_pedido_chamado">
    <indexes>
      <index name="IDX_E05E0522E2DBA323" columns="id_pedido"/>
      <index name="IDX_E05E0522E59FAF1F" columns="id_chamado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="rel_pedido_chamado_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <many-to-one field="idChamado" target-entity="Chamado" fetch="LAZY">
      <join-columns>
        <join-column name="id_chamado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPedido" target-entity="Pedido" fetch="LAZY">
      <join-columns>
        <join-column name="id_pedido" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
